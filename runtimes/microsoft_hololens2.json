{
    "$schema": "../schema.json",
    "name": "Windows Mixed Reality - HoloLens 2",
    "conformance_submission": 1,
    "platform": "Windows (HoloLens 2)",
    "vendor": "Microsoft Corporation",
    "extensions": [
        "XR_KHR_composition_layer_color_scale_bias",
        "XR_KHR_composition_layer_depth",
        "XR_KHR_D3D11_enable",
        "XR_KHR_D3D12_enable",
